  <step>
    <name>Get study names</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>I2B2METADATA</connection>
    <sql>SELECT
 replace(replace(c_fullname,c_name,&apos;&apos;),&apos;\\&apos;,&apos;\&apos;) || &apos;;&apos; || C_NAME || &apos;;&apos; || SOURCESYSTEM_CD as STUDY_NAME
FROM I2B2
where c_fullname in
(select min(c_fullname) from i2b2
 where sourcesystem_cd is not null
   and c_fullname not like &apos;%\SECURITY\&apos; escape &apos;&apos;
   group by sourcesystem_cd)
order by c_fullname
</sql>

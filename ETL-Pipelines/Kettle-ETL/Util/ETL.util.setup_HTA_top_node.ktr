  <step>
    <name>Get top nodes from i2b2</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>I2B2METADATA</connection>
    <sql>SELECT
 C_FULLNAME
FROM I2B2
where sourcesystem_cd is null
and c_fullname not like &apos;%\SECURITY\&apos; escape &apos;&apos;
order by c_fullname
</sql>

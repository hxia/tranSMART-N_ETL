        <jsScript_script>&#47;&#47;Script here

var date_error=&quot;&quot;;
var output_string=&quot;&quot;;
var dateVar=new Date();
var date_status=&quot;Good&quot;;
var save_format=replace(input_format,&quot;DATE:&quot;,&quot;&quot;)+&quot;;&quot;

var wrk_format=&quot;&quot;;
var wrk_string=&quot;&quot;;
var x_pos=0;
var mult_format_pos=0;
var last_pos=0;
var format_len=save_format.length;


	output_string=input_string;

	if ( input_string==null )
	{
		date_status=&quot;&quot;;
	}
	else
	{
		do
		{
			mult_format_pos=indexOf(save_format,&quot;;&quot;,last_pos);
			wrk_format=substr(save_format,last_pos,mult_format_pos-last_pos);
			x_pos=indexOf(wrk_format,&quot;X&quot;);
			wrk_format=replace(wrk_format,&quot;X&quot;,&quot; &quot;);
			wrk_string=input_string;
			date_status=&quot;Good&quot;;

			if ( x_pos&gt;0 )
			{
				if ( input_string.length &lt;= x_pos)
				{
					wrk_string=input_string;
				}
				else
				{
				wrk_string=substr(input_string,0,x_pos)+&quot; &quot;+substr(input_string,x_pos+1);
				}
			}

			try
			{
				var inputParser= new java.text.SimpleDateFormat(wrk_format); &#47;&#47;Must use fully qualified java class
				dateVar=inputParser.parse(wrk_string); &#47;&#47;just like how you would do in java

				if ( input_date_type==&quot;T&quot;)
				{
					output_string=date2str(dateVar,&quot;HH:mm&quot;);

				}
				else
				{
					if ( input_date_type==&quot;DL&quot; )
					{

						output_string=date2str(dateVar,&quot;yyyy-MM-dd HH:mm&quot;);
					}
					else
					{
						output_string=date2str(dateVar,&quot;yyyyMMdd.HHmm&quot;);
					}
				}
				date_status=&quot;Good&quot;;

			}
			catch (e) 
			{

				date_status=&quot;Error&quot;;
				date_error=e;
				if ( input_date_type==&quot;DL&quot; )
				{
					output_string=null;
				}
				if ( input_date_type==&quot;T&quot; )
				{
					output_string=null;
				}
			}

			if ( date_status==&quot;Good&quot; )
			{
				break;
			}
			last_pos=mult_format_pos+1;

		}
		while ( last_pos &lt; format_len )
	}


</jsScript_script>

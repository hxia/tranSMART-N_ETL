    <connection>I2B2DEMODATA</connection>
    <sql>select p.modifier_path
	  ,count(*) as nbr_children
   ,length(p.modifier_path)-length(replace(p.modifier_path,&apos;\&apos;,&apos;&apos;))-2 as node_level
from i2b2demodata.modifier_dimension p
    ,i2b2demodata.modifier_dimension c
where c.modifier_path like p.modifier_path || &apos;%&apos; escape &apos;&apos;
group by p.modifier_path</sql>
